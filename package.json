{
  "name": "tosidrop-fe",
  "version": "1.0.0",
  "description": "Tosidrop frontend: Express router + React client",
  "repository": {
    "type": "git",
    "url": "git://github.com/TosiDrop/vm-frontend.git"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "main": "index.ts",
  "scripts": {
    "build": "npm install",
    "build-client": "cd client && npm run build",
    "clean": "rm -rf node_modules",
    "clean-client": "cd client && npm run clean",
    "start": "node --inspect -r ts-node/register index.ts",
    "start-dev": "nodemon index.ts",
    "test": "exit 0",
    "test-client": "cd client && npm run test",
    "dev": "concurrently \"npm run start-dev\" \"cd client && npm start\""
  },
  "proxy": "http://localhost:3000",
  "keywords": [],
  "dependencies": {
    "axios": "^0.26.0",
    "concurrently": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.16"
  }
}
